    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Emergency Phone Numbers"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyAHFeYmuLkhc8JVA_FRlWPt7OwmUnFNHw4" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ScrollingFlags"
            android:label="@string/title_activity_scrolling_flags"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".CountrySelection"
            android:label="@string/title_activity_country_selection"></activity>
        <activity android:name=".CountrySelector"></activity>
